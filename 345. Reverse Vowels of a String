//9ms

class Solution {
public:
    string reverseVowels(string s) {
        int begin = 0, end = s.length()-1;
        char vowelsMap[127] = {0};
        vowelsMap['a'] = 1;
        vowelsMap['e'] = 1;
        vowelsMap['i'] = 1;
        vowelsMap['o'] = 1;
        vowelsMap['u'] = 1;
        vowelsMap['A'] = 1;
        vowelsMap['E'] = 1;
        vowelsMap['I'] = 1;
        vowelsMap['O'] = 1;
        vowelsMap['U'] = 1;
        char temp;
        while(vowelsMap[s[begin]] == 0) begin++;
        while(vowelsMap[s[end]] == 0) end--;
        while (begin < end)
        {
            temp = s[begin];
            s[begin] = s[end];
            s[end] = temp;
            begin++;
            end--;
            
            while(vowelsMap[s[begin]] == 0) begin++;
            while(vowelsMap[s[end]] == 0) end--;
        }
        return s;
    }
};
